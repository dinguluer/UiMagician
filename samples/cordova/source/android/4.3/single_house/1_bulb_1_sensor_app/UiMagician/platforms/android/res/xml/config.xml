<?xml version='1.0' encoding='utf-8'?>
<widget id="com.DineshGuleria.UiMagician" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WebSocket">
        <param name="android-package" value="com.ququplay.websocket.WebSocket" />
    </feature>
    <splash density="land-hdpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="land-ldpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="land-mdpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="land-xhdpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="port-hdpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="port-ldpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="port-mdpi" src="res/drawable-xhdpi/screen.png" />
    <splash density="port-xhdpi" src="res/drawable-xhdpi/screen.png" />
    <name>UiMagician</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" height="57" src="www/img/icon.png" width="57" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="4000" />
</widget>
