<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Dinesh.test" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>test</name>
    <description>
        A sample Demo UiMagician application that responds to the button pressed.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        UiMagician Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/icon.png" width="57" height="57" density="mdpi" />

    <feature name="SplashScreen">
       <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>

    <platform name="android">
          <icon src="res/drawable-ldpi/screen.png" density="ldpi" />
          <icon src="res/drawable-mdpi/screen.png" density="mdpi" />
          <icon src="res/drawable-hdpi/screen.png" density="hdpi" />
          <icon src="res/drawable-xhdpi/screen.png" density="xhdpi" />
    </platform>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />

</widget>
